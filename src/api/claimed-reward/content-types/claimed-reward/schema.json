{
	"kind": "collectionType",
	"collectionName": "claimed_rewards",
	"info": {
		"singularName": "claimed-reward",
		"pluralName": "claimed-rewards",
		"displayName": "Claimed Reward",
		"description": "Rewards claimed by participants"
	},
	"options": {
		"draftAndPublish": false
	},
	"pluginOptions": {},
	"attributes": {
		"participant": {
			"type": "relation",
			"relation": "manyToOne",
			"target": "api::participant.participant",
			"inversedBy": "claimedRewards"
		},
		"reward": {
			"type": "relation",
			"relation": "manyToOne",
			"target": "api::reward.reward"
		},
		"business": {
			"type": "relation",
			"relation": "manyToOne",
			"target": "api::business.business",
			"required": false
		},
		"pointsSpent": {
			"type": "integer",
			"required": true
		},
		"claimedAt": {
			"type": "datetime",
			"required": true
		},
		"expiresAt": {
			"type": "datetime",
			"required": false
		},
		"qrCode": {
			"type": "string",
			"unique": true,
			"required": false
		},
		"passUrl": {
			"type": "string",
			"required": false
		},
		"isRedeemed": {
			"type": "boolean",
			"default": false,
			"required": false
		},
		"redeemedAt": {
			"type": "datetime",
			"required": false
		},
		"status": {
			"type": "enumeration",
			"enum": ["active", "redeemed", "expired"],
			"default": "active"
		}
	}
}
